config_version=5

[application]

config/name="MonsterChromatic"
run/main_scene="res://scenes/ui/MainMenu.tscn"
config/features=PackedStringArray("4.5", "Mobile")
config/icon="res://icon.svg"

[autoload]

InputManager="*res://autoload/InputManager.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=1280
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[dotnet]

project/assembly_name="MonsterChromatic"

